                            <td><input type="button" class="btn btn-warning" value="N"></td>
                        }
                    }

                </tr>
                <tr>
                    <td>
                        <strong>Wirtschaft</strong>
                    </td>

                    @foreach (var questLog in Model
                        .Where(logEntry => logEntry.Question.Category.Name.Equals("Wirtschaft"))
